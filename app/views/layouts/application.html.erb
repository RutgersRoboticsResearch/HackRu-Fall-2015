<html>
<head>
  <title>HACKRU | Spring 2015</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div id="nav">
      <div id="cent">
        <img src="assets/brand.png" width="40px"/>
        <img src="assets/about.png"/>
        <img src="assets/sponsors.png"/>
        <img src="assets/faq.png"/>
        <img src="assets/location.png"/>
        <img src="assets/contact.png"/>
   <ul id="signup">
    <% if user_signed_in? %>
    <li><%= link_to current_user.full_name, edit_user_registration_path %></li>
    <%= link_to "Log Out", logout_path, class: 'registerBtn2 registerBtn-1' %>
    <% else %>
    <%= link_to "Sign up", register_path, class: 'registerBtn2 registerBtn-1' %>
    <%= link_to "Sign in", login_path, class: 'registerBtn2 registerBtn-1'%>
    <% end %>
  </ul>
        </div>
      </div>
    <%= yield %>
</body>
</html>